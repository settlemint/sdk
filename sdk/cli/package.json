{
  "name": "@settlemint/sdk-cli",
  "description": "SettleMint SDK, integrate SettleMint into your application with ease.",
  "version": "0.5.0",
  "type": "module",
  "private": false,
  "license": "FSL-1.1-MIT",
  "author": {
    "name": "SettleMint",
    "email": "support@settlemint.com",
    "url": "https://settlemint.com"
  },
  "homepage": "https://github.com/settlemint/sdk/blob/main/packages/sdk/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/settlemint/sdk.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/settlemint/sdk/issues",
    "email": "support@settlemint.com"
  },
  "files": ["dist"],
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.mjs"
    }
  },
  "bin": {
    "settlemint": "dist/cli.js"
  },
  "scripts": {
    "build": "bun run build.ts",
    "dev": "tsup-node --watch",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "publish-npm": "bun publish --tag ${TAG} --access public || exit 0"
  },
  "dependencies": {
    "hardhat": "2.22.17"
  },
  "devDependencies": {
    "@types/semver": "7.5.8",
    "is-in-ci": "1.0.0",
    "semver": "7.6.3",
    "slugify": "1.6.6",
    "yoctocolors": "2.1.1",
    "@commander-js/extra-typings": "13.0.0",
    "@inquirer/confirm": "5.1.1",
    "@inquirer/input": "4.1.1",
    "@inquirer/password": "4.0.4",
    "@inquirer/select": "4.0.4",
    "@settlemint/sdk-js": "workspace:*",
    "@settlemint/sdk-utils": "workspace:*",
    "get-tsconfig": "4.8.1",
    "giget": "1.2.3"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=20"
  }
}
