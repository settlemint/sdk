{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "//#lint": {
      "cache": false
    },
    "//#format": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^test", "build"]
    },
    "attw": {
      "dependsOn": ["build"]
    },
    "publint": { "dependsOn": ["build"] }
  }
}
