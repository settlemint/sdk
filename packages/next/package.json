{
  "name": "@settlemint/sdk-next",
  "description": "SettleMint SDK, integrate SettleMint into your application with ease.",
  "version": "0.0.1",
  "type": "module",
  "private": false,
  "license": "FSL-1.1-MIT",
  "author": {
    "name": "SettleMint",
    "email": "support@settlemint.com",
    "url": "https://settlemint.com"
  },
  "homepage": "https://github.com/settlemint/sdk/blob/main/packages/sdk/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/settlemint/sdk.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/settlemint/sdk/issues",
    "email": "support@settlemint.com"
  },
  "files": ["dist"],
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch",
    "publint": "publint run --strict",
    "attw": "attw --pack .",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {},
  "dependencies": {
    "@rainbow-me/rainbowkit": "2.1.6",
    "@settlemint/sdk-config": "workspace:*",
    "abitype": "1.0.6",
    "graphql-request": "7.1.0",
    "openapi-fetch": "0.12.0",
    "path-to-regexp": "7.2.0",
    "viem": "2.21.7",
    "wagmi": "2.12.11"
  },
  "peerDependencies": {
    "next": ">=14",
    "react": ">=18"
  },
  "engines": {
    "node": ">=20"
  }
}
