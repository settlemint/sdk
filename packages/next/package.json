{
  "name": "@settlemint/sdk-next",
  "description": "SettleMint SDK for Next.js, integrate SettleMint into your application with ease.",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "private": false,
  "license": "MIT",
  "author": {
    "name": "SettleMint",
    "email": "support@settlemint.com",
    "url": "https://settlemint.com"
  },
  "homepage": "https://github.com/settlemint/sdk/blob/main/packages/next/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/settlemint/sdk.git",
    "directory": "packages/next"
  },
  "bugs": {
    "url": "https://github.com/settlemint/sdk/issues",
    "email": "support@settlemint.com"
  },
  "files": ["dist"],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup-node src/index.ts --format esm,cjs --dts",
    "dev": "tsup-node src/index.ts --format esm,cjs --dts --watch",
    "publint": "publint run --strict",
    "attw": "attw --pack ."
  },
  "dependencies": {
    "@settlemint/sdk-config": "workspace:*",
    "@settlemint/sdk-react": "workspace:*"
  },
  "devDependencies": {
    "tsup": "8.2.4",
    "next": "14.2.5"
  },
  "peerDependencies": {
    "next": "^13.5.4 || ^14.0.3 || >=15.0.0-rc",
    "@tanstack/react-query": "^5",
    "@tanstack/react-query-devtools": "^5"
  }
}
