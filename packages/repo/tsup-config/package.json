{
  "name": "@repo/tsup-config",
  "version": "0.0.0",
  "private": true,
  "files": ["tsup-cli.ts", "tsup.config.ts"],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/tsup.config.d.mts",
        "default": "./dist/tsup.config.mjs"
      },
      "require": {
        "types": "./dist/tsup.config.d.ts",
        "default": "./dist/tsup.config.js"
      }
    },
    "./cli": {
      "import": {
        "types": "./dist/tsup-cli.config.d.mts",
        "default": "./dist/tsup-cli.config.mjs"
      },
      "require": {
        "types": "./dist/tsup-cli.config.d.ts",
        "default": "./dist/tsup-cli.config.js"
      }
    }
  },
  "devDependencies": {
    "tsup": "8.2.4"
  },
  "scripts": {
    "build": "tsup-node tsup-cli.config.ts tsup.config.ts",
    "dev": "tsup-node tsup-cli.config.ts tsup.config.ts --watch"
  }
}
