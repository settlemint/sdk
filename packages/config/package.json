{
  "name": "@settlemint/sdk-config",
  "description": "SettleMint SDK, integrate SettleMint into your application with ease.",
  "version": "0.0.1",
  "type": "module",
  "private": false,
  "license": "FSL-1.1-MIT",
  "author": {
    "name": "SettleMint",
    "email": "support@settlemint.com",
    "url": "https://settlemint.com"
  },
  "homepage": "https://github.com/settlemint/sdk/blob/main/packages/sdk/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/settlemint/sdk.git",
    "directory": "packages/sdk-config"
  },
  "bugs": {
    "url": "https://github.com/settlemint/sdk/issues",
    "email": "support@settlemint.com"
  },
  "files": ["dist"],
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch",
    "publint": "publint run --strict",
    "attw": "attw --pack .",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {},
  "dependencies": {
    "dotenv": "^16",
    "ts-deepmerge": "^7",
    "zod": "^3"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=20"
  }
}
