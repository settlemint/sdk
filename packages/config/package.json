{
  "name": "@settlemint/sdk-config",
  "description": "SettleMint SDK (configuration utility)",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "private": false,
  "license": "MIT",
  "author": {
    "name": "SettleMint",
    "email": "support@settlemint.com",
    "url": "https://settlemint.com"
  },
  "homepage": "https://github.com/settlemint/sdk/blob/main/packages/config/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/settlemint/sdk.git",
    "directory": "packages/config"
  },
  "bugs": {
    "url": "https://github.com/settlemint/sdk/issues",
    "email": "support@settlemint.com"
  },
  "files": ["dist"],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup-node src/index.ts --format cjs,esm --dts",
    "dev": "tsup-node src/index.ts --format cjs,esm --dts --watch",
    "publint": "publint run --strict",
    "attw": "attw --pack ."
  },
  "devDependencies": {
    "tsup": "8.2.4"
  },
  "dependencies": {
    "cosmiconfig": "9.0.0",
    "dotenv": "16.4.5",
    "ts-deepmerge": "7.0.1",
    "zod": "3.23.8"
  },
  "peerDependencies": {}
}
