{
  "name": "@settlemint/btp-sdk-react",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": false,
  "license": "MIT",
  "author": {
    "name": "SettleMint",
    "email": "support@settlemint.com",
    "url": "https://settlemint.com"
  },
  "homepage": "https://github.com/settlemint/btp-sdk/blob/main/packages/react/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/settlemint/btp-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/settlemint/btp-sdk/issues",
    "email": "support@settlemint.com"
  },
  "files": ["dist"],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup-node src/index.ts --format esm,cjs --dts",
    "dev": "tsup-node src/index.ts --format esm,cjs --dts --watch",
    "lint:publint": "publint run",
    "lint:attw": "attw --pack ."
  },
  "devDependencies": {
    "tsup": "8.2.4"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18 || >=19.0.0-beta",
    "react-dom": ">=18 || >=19.0.0-beta"
  }
}
