  "scripts": {
    "dev": "next dev --port 3330",
    "connect": "op run --env-file=\".env.connect.1p\" -- bun run --bun ../../packages/sdk/src/cli/cli.ts connect",
    "codegen": "bun run --bun ../../packages/sdk/src/cli/cli.ts codegen",
    "deploy": "cd contracts && bun run --bun ../../../packages/sdk/src/cli/cli.ts scs hardhat deploy remote",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "forge:dependencies": "cd contracts && forge soldeer update",
    "forge:build": "cd contracts && bun run --bun ../../../packages/sdk/src/cli/cli.ts forge build",
    "forge:test": "cd contracts && bun run --bun ../../../packages/sdk/src/cli/cli.ts forge test",
    "forge:deploy": "cd contracts && bun run --bun ../../../packages/sdk/src/cli/cli.ts forge create src/AssetERC1155.sol:AssetERC1155",
    "hasura:migrate": "cd database && hasura metadata apply && hasura migrate apply --all-databases && hasura metadata reload",
    "hasura:console": "cd database && hasura console"
  },
