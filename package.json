{
  "name": "sdk",
  "private": true,
  "version": "0.4.7",
  "license": "FSL-1.1-MIT",
  "scripts": {
    "clean": "git clean -fdX && find . -type f \\( -name 'pnpm-lock.yaml' -o -name 'package-lock.json' -o -name 'yarn.lock' -o -name 'bun.lockb' \\) -exec rm -f {} + && bun install",
    "pkg:build": "turbo pkg:build",
    "pkg:test": "turbo pkg:test",
    "pkg:test:coverage": "turbo pkg:test:coverage",
    "pkg:dev": "turbo pkg:dev",
    "lint": "biome lint --write --unsafe .",
    "format": "biome format --write .",
    "pkg:publint": "turbo pkg:publint",
    "pkg:attw": "turbo pkg:attw",
    "knip": "knip"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.15.4",
    "@biomejs/biome": "1.8.3",
    "@tsconfig/next": "2.0.3",
    "@tsconfig/node22": "22.0.0",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.3.0",
    "knip": "5.27.2",
    "lefthook": "1.7.13",
    "publint": "0.2.10",
    "tsup": "8.2.4",
    "turbo": "2.0.14",
    "typescript": "5.5.4",
    "@vitest/coverage-v8": "2.0.5",
    "vitest": "2.0.5"
  },
  "engines": {
    "node": "22.6.0"
  },
  "packageManager": "bun@1.1.22",
  "workspaces": [
    "packages/*",
    "packages/sdk/templates/*"
  ],
  "trustedDependencies": [
    "@biomejs/biome",
    "esbuild",
    "lefthook"
  ]
}
